<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sitting_room.golfsg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.redballgolf.golfSG.registerLogin.Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.redballgolf.golfSG.leagues.IndividualLeague"
            android:label="@string/title_activity_individual_league"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.redballgolf.golfSG.leagues.LeaguesMenu"
            android:label="@string/title_activity_leagues_menu"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>